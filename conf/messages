#
# General Error Messages
#
BAD_PARAMETER_VALUE = Valor incorrecto en el parametro ''{0}''
MISSING_PARAMETER = Falta el parametro ''{0}''
NOT_SUPPORTED=No soportado 
INTERNAL_SERVER_ERROR=Error interno del servidor
NOT_IMPLEMENTED=No esta implementado 
UNAUTHORIZED=No tiene permiso
BAD_JSON= Error manipulando JSON

#
# Wrraped Error Message
#
WRAPPED_EXCEPTION= Envoltorio para otra Exception
 




#
# Common Error Messages
#
ACCOUNT_NOT_FOUND= Account with {0}:{1} not found
USER_NOT_FOUND= User with {0}:{1} not found
ALERT_NOT_FOUND= Alert with {0}:{1} not found
CREDENTIAL_NOT_FOUND= Credential with {0}:{1} not found 
SESSION_NOT_FOUND= Session with {0}:{1} not found 
INVALID_VALIDATION_KEY = Key is not valid
NO_AUTH_METHOD_DEFINED= No authentication method defined
HAZELCAST= Hazelcast error 
USER_NOT_PART_OF_ACCOUNT= User not part of account
USER_ALREADY_EXISTS= User with {0}:{1} already exists
EMAIL_EXISTS= Email {0} exists
PHONE_EXISTS= Phone {0} exists
SETTING_SCHEMA_ERROR= Error with settings schema











