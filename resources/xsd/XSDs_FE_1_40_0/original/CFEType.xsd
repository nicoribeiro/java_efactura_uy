					<xs:restriction base="xs:string">
						<xs:maxLength value="1"/>
						<xs:enumeration value="R"/>
						<xs:enumeration value="A"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="NomItem">
				<xs:annotation>
					<xs:documentation>Nombre del Item</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="80"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
